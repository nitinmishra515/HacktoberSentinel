{
  "name": "hacktober-sentinel",
  "version": "1.0.1",
  "description": "HacktoberSentinel GitHub Action to flag Hacktoberfest spam PRs.",
  "main": "dist/index.js",
  "type": "commonjs",
  "scripts": {
    "build": "pnpm exec ncc build index.js -o dist",
    "prepare": "pnpm run build",
    "lint": "pnpm exec eslint . --ext .js",
    "test": "pnpm exec jest --runInBand"
  },
  "keywords": [
    "github-action",
    "hacktoberfest",
    "spam"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "1.11.1",
    "@actions/github": "6.0.1"
  },
  "devDependencies": {
    "@vercel/ncc": "0.38.4",
    "eslint": "8.57.1",
    "jest": "29.7.0"
  },
  "jest": {
    "testEnvironment": "node"
  }
}
